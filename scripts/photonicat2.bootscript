part uuid mmc ${devnum}:2 uuid

setenv bootargs "console=ttyS0,1500000 console=tty1 earlycon=uart8250,mmio32,0x2ad40000 video=HDMI-A-1:e root=PARTUUID=${uuid} rw rootwait"

load mmc ${devnum}:1 ${fdt_addr_r} rockchip.dtb
load mmc ${devnum}:1 ${kernel_addr_r} Image
load mmc ${devnum}:1 ${ramdisk_addr_r} initrd.img

booti ${kernel_addr_r} ${ramdisk_addr_r} ${fdt_addr_r}
